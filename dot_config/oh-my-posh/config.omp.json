  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:background",
          "foreground": "p:os",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ÓÇ±",
          "type": "os"
        },
        {
          "background": "p:background",
          "foreground": "p:catppuccin-red",
          "style": "plain",
          "template": " ÔÉß <#bac2de>ÓÇ±</>",
          "type": "root",
          "comment": "Show when the current user is root or when in an elevated shell (Windows)"
        },
        {
          "background": "p:background",
          "foreground": "p:path",
          "properties": {
            "style": "powerlevel",
            "edge_format": "<b>%s</b>",
            "max_width": 80
          },
          "style": "plain",
          "template": " ÔÅº {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:background",
          "foreground": "p:catppuccin-green",
          "style": "plain",
          "template": "<#bac2de>ÓÇ±</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} ÓÇ± ÔÅÜ {{ .Staging.String }}{{ end }}{{ if .Working.Changed }} ÓÇ± ÔÅÑ {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} ÓÇ± Ó≠ã {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_max_length": 0,
            "branch_icon": "ÔÑ¶ ",
            "branch_identical_icon": "ÔëÑ",
            "branch_gone_icon": "Ó™∏",
            "branch_behind_icon": "ÔÅ£",
            "branch_ahead_icon": "ÔÅ¢",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "feature/*": "üöÄ ",
              "feat/*": "üöÄ ",
              "work/henri.buyse/*": "üî® ",
              "work/*": "üî® "
            },
            "tag_icon": "Ôêí ",
            "rebase_icon": "Óú® ",
            "git_icon": "Óóª ",
            "merge_icon": "Óúß ",
            "commit_icon": "Ôêó ",
            "revert_icon": "ÔÉ¢ ",
            "no_commits_icon": "Ôñî ",
            "cherry_pick_icon": "Óäõ ",
            "full_branch_path": true
          },
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "p:background",
          "style": "plain",
          "template": "ÓÇ∞",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "<#313244,transparent>¬∑</>",
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:background",
          "style": "plain",
          "template": "ÓÇ≤",
          "type": "text"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:catppuccin-subtext0",
          "background": "p:background",
          "template": " {{ .FormattedMs }} Û±¶ü <#bac2de>ÓÇ≥</>",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "type": "cmake",
          "style": "plain",
          "foreground": "p:project",
          "background": "p:background",
          "template": " {{ .Full }} Óòû Óòù <#bac2de>ÓÇ≥</>"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:project",
          "background": "p:background",
          "template": " {{ if .Venv }}{{ .Venv }}{{ else }}{{ .Full }}{{ end }} Óàµ <#bac2de>ÓÇ≥</>"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:project",
          "background": "p:background",
          "template": " {{ .Full }} Óû® <#bac2de>ÓÇ≥</>"
        },
        {
          "type": "lua",
          "style": "plain",
          "foreground": "p:project",
          "background": "p:background",
          "template": " {{ .Executable }} {{ .Full }} Óò† <#bac2de>ÓÇ≥</>"
        },
        {
          "background": "p:background",
          "foreground": "p:status-fg-success",
          "foreground_templates": ["{{ if .Error }}p:status-fg-error{{ end }}"],
          "style": "plain",
          "template": "{{ if .Error }}{{ .String }}ÔÅ± {{ end }}<#bac2de>ÓÇ≥</>",
          "properties": {
            "status_template": " {{ reason .Code }} "
          },
          "type": "status"
        },
        {
          "background": "p:background",
          "foreground": "p:session",
          "style": "plain",
          "template": " {{ .UserName }}@{{ .HostName }} ÔÄá <#bac2de>ÓÇ≥</>",
          "type": "session"
        },
        {
          "background": "p:background",
          "foreground": "p:time",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ÔÄó ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:status-fg-success",
          "foreground_templates": ["{{ if .Error }}p:status-fg-error{{ end }}"],
          "background": "transparent",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
