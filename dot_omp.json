{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "p10k-bg": "#313244",
    "background": "p:p10k-bg",
    "separator-fg": "#EEEEEE",
    "session-fg": "#D7AF00",
    "time-fg": "#A6ADC8",
    "git-fg": "76",
    "status-bg": "transparent",
    "status-fg-success": "#5faf00",
    "status-fg-error": "#FF5252"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:background",
          "foreground": "p:separator-fg",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ÓÇ±",
          "type": "os"
        },
        {
          "background": "p:background",
          "foreground": "#00AAFF",
          "style": "plain",
          "template": " ÔÉß ",
          "type": "root",
          "comment": "Show when the current user is root or when in an elevated shell (Windows)"
        },
        {
          "background": "p:background",
          "foreground": "#26C6DA",
          "properties": {
            "style": "powerlevel",
            "edge_format": "<b>%s</b>",
            "max_width": 80
          },
          "style": "plain",
          "template": " ÔÅº {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:background",
          "foreground": "p:git-fg",
          "style": "plain",
          "template": "<#EEEEEE>ÓÇ±</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} ÓÇ± ÔÅÜ {{ .Staging.String }}{{ end }}{{ if .Working.Changed }} ÓÇ± ÔÅÑ {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} ÓÇ± Ó≠ã {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_max_length": 0,
            "branch_icon": "ÔÑ¶ ",
            "branch_identical_icon": "ÔëÑ",
            "branch_gone_icon": "Ó™∏",
            "branch_behind_icon": "ÔÅ£",
            "branch_ahead_icon": "ÔÅ¢",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "feature/*": "üöÄ ",
              "feat/*": "üöÄ "
            },
            "full_branch_path": true
          },
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "p:p10k-bg",
          "style": "plain",
          "template": "ÓÇ∞",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "<#313244,transparent>¬∑</>",
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:p10k-bg",
          "style": "plain",
          "template": "ÓÇ≤",
          "type": "text"
        },
        {
          "background": "p:background",
          "foreground": "p:status-fg-success",
          "foreground_templates": ["{{ if .Error }}p:status-fg-error{{ end }}"],
          "style": "plain",
          "template": "{{ if ne .Code 0 }} {{ reason .Code }} {{ end }}<#eeeeee>ÓÇ≥</>",
          "type": "status"
        },
        {
          "background": "p:background",
          "foreground": "#D7AF00",
          "style": "plain",
          "template": " {{ .UserName }}@{{ .HostName }} <#eeeeee>ÓÇ≥</>",
          "type": "session"
        },
        {
          "background": "p:background",
          "foreground": "p:time",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ÔÄó ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:status-fg-success",
          "foreground_templates": ["{{ if .Error }}p:status-fg-error{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
